<?xml version="1.0"?>
<!--
     Test file for visualisation
-->
<enp version="0.0.2">
  <simulator
    wd="/Users/zahedi/projects/builds/yars-build"
    xml="xml/braitenberg_tcpip.xml"
    nr="4"/>
  <evaluation>
    <lifetime iterations="500"/>
  </evaluation>
  <reproduction>
    <population size="30"/>
    <selection  pressure="0.1"/>
  </reproduction>
  <evolution>
    <node cost="0.1">
      <modify probability="0.11" maximum="0.12" delta="0.13"/>
      <add    probability="0.14" maximum="0.15"/>
      <delete probability="0.16"/>
    </node>
    <edge cost="0.2">
      <modify probability="0.21" maximum="0.22" delta="0.23"/>
      <add    probability="0.24" maximum="0.25"/>
      <delete probability="0.26"/>
    </edge>
  </evolution>
  <configuration>
    <module name="controller">

      <node type="sensor" label="left sensors">
        <position x="0.5" y="0" z="0"/>
        <transferfunction name="id"/>
        <bias value="0"/>
      </node>
      <node type="sensor" label="right sensors">
        <position x="-0.5" y="0" z="0"/>
        <transferfunction name="id"/>
        <bias value="0"/>
      </node>

      <node type="actuator" label="left actuator">
        <position x="0.5" y="-1" z="0"/>
        <transferfunction name="tanh"/>
        <bias value="0"/>
      </node>
      <node type="actuator" label="right actuator">
        <position x="-0.5" y="-1" z="0"/>
        <transferfunction name="tanh"/>
        <bias value="0"/>
      </node>

      <!-- <edge source="left sensors"  destination="right actuator" weight="-3.0"/> -->
      <!-- <edge source="right sensors" destination="left actuator"  weight="-3.0"/> -->

    </module>
  </configuration>
</enp>
